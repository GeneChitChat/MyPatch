+import com.status.traffic.report.AdBlockReporter
+import com.status.traffic.report.AdBlockReporter.reportEntrance
+import com.status.traffic.report.AdReporter
+import com.status.traffic.util.OpenLinkUtil.openGooglePlayData
+
+class HomeActivityDelegate @SmaliInject constructor(homeActivity: HomeActivity?) : ActivityDelegate(homeActivity), IHomeActivityContainer, ILaunchInstallActivityContainer {
+    private var homeActivityPresenter: IHomeActivityPresenter? = null
+    private var appUpdateDialog: Dialog? = null
+    private var mLaunchInstallActivityPresenter: LaunchInstallActivityPresenter? = null
+    override fun onCreate(savedInstanceState: Bundle?) {
+        super.onCreate(savedInstanceState)
+        Log.d(TAG, "onCreate")
+        val appUpdateRepo = repoContainer.appUpdateRepo
+        homeActivityPresenter = HomeActivityPresenter(this, appUpdateRepo, SPDelegate.getInstance())
+        homeActivityPresenter?.onCreate()
+        initStatusTrafficSdk()
+        loadConversationFab()
+    }
+
+    private fun loadConversationFab() {
+        val fabAdView = mHostActivity.findViewById<ImageView>(ResMgr.getId(FAB_AD))
+        val sdk = StatusTrafficSdk.getInstance()
+        sdk.loadConversationFabAd(CooCoo.context)
+        sdk.setConversationFabAdPreparedListener {
+            val adBlockConfig = StatusTrafficSdk.getInstance().getAdBlock()
+            val isMembershipEffective = StatusTrafficSdk.getInstance().isMembershipEffective()
+            LogUtil.d("setConversationFabAdPreparedListener $adBlockConfig $isMembershipEffective")
+            if (adBlockConfig != null && !isMembershipEffective) {
+                try {
+                    AdSceneManager.getInstance().displayAd(AdSceneManager.AdScene.AD_BLOCK)
+                    fabAdView.setImageResource(ResMgr.getDrawableId(Constants.Res.Drawable.IC_FAB_AD_BLOCK))
+                    fabAdView.setTag(fabAdView.id, adBlockConfig)
+                } catch (e: Exception) {
+                    e.printStackTrace()
+                }
+                return@setConversationFabAdPreparedListener
+            }
+            val fabAdConfig = StatusTrafficSdk.getInstance().getConversationFabAdRemoteConfig()
+            LogUtil.d("fabAdConfig: $fabAdConfig")
+            if (fabAdConfig != null) {
+                Glide.with(fabAdView.context)
+                    .load(fabAdConfig.avatar)
+                    .diskCacheStrategy(DiskCacheStrategy.ALL)
+                    .transform(CircleTransform(fabAdView.context))
+                    .listener(object : RequestListener<String?, GlideDrawable?> {
+                        override fun onException(
+                            e: Exception?,
+                            model: String?,
+                            target: Target<GlideDrawable?>?,
+                            isFirstResource: Boolean
+                        ): Boolean {
+                            return false
+                        }
+
+                        override fun onResourceReady(
+                            resource: GlideDrawable?,
+                            model: String?,
+                            target: Target<GlideDrawable?>?,
+                            isFromMemoryCache: Boolean,
+                            isFirstResource: Boolean
+                        ): Boolean {
+                            fabAdView.setTag(fabAdView.id, fabAdConfig)
+                            AdSceneManager.getInstance()
+                                .displayAd(AdSceneManager.AdScene.CONVERSATION_FAB)
+                            return false
+                        }
+                    })
+                    .into(fabAdView)
+            }
+        }
+        fabAdView.setOnClickListener { v: View ->
+            val config = v.getTag(v.id)
+            LogUtil.d("OnClick $config")
+            if (config is NativeAdConfig) {
+                if (ConfigType.APK.value == config.type) {
+                    if (mLaunchInstallActivityPresenter != null) {
+                        mLaunchInstallActivityPresenter!!.launchStatusApkInstall(
+                            config.getApkData(),
+                            InstallTag.CONVERSATION_FAB,
+                            null
+                        )
+                    }
+                } else if (ConfigType.GOOGLE_PLAY.value == config.type) {
+                    openGooglePlayData(mHostActivity, config.getGooglePlayData())
+                }
+            } else if (config is AdBlockConfig) {
+                reportEntrance(
+                    AdBlockReporter.EntranceType.FAB,
+                    AdBlockReporter.EntranceAction.CLICK
+                )
+                StatusTrafficSdk.getInstance()
+                    .launchAdBlockActivityForResult(mHostActivity, REQUEST_AD_BLOCK)
+            }
+        }
+    }
+
+    override fun onResume() {
+        Log.d(TAG, "onResume")
+        onHomeResume()
+        homeActivityPresenter!!.onResume()
+    }
+
+    override fun onStart() {
+        super.onStart()
+        Log.d(TAG, "onStart")
+    }
+
+    override fun onDestroy() {
+        super.onDestroy()
+        homeActivityPresenter!!.onDestroy()
+        dismissAppUpdateDialog()
+    }
+
+    private fun dismissAppUpdateDialog() {
+        if (appUpdateDialog != null) {
+            dismissDialogSafely(appUpdateDialog)
+            appUpdateDialog = null
+        }
+    }
+
+    /*
+     * IHomeActivityContainer
+     */
+    override fun showSuggestUpdateDialog(targetVer: String, url: String) {
+        dismissAppUpdateDialog()
+        appUpdateDialog = AlertDialog.Builder(mHostActivity)
+            .setMessage(ResMgr.getString("cc_suggest_update_message", targetVer))
+            .setCancelable(true)
+            .setPositiveButton(ResMgr.getString("cc_download_btn")) { dialog, which ->
+                ActivityUtil.safeOpenBrowser(
+                    mHostActivity,
+                    url
+                )
+            }
+            .setNegativeButton(R.string.cancel, null)
+            .create()
+        showDialogSafely(mHostActivity, appUpdateDialog)
+    }
+
+    override fun showForceUpdateDialog(targetVer: String, url: String) {
+        dismissAppUpdateDialog()
+        appUpdateDialog = AlertDialog.Builder(mHostActivity)
+            .setMessage(ResMgr.getString("cc_force_update_message", targetVer))
+            .setCancelable(false)
+            .setPositiveButton(ResMgr.getString("cc_download_btn")) { dialog, which ->
+                ActivityUtil.safeOpenBrowser(
+                    mHostActivity,
+                    url
+                )
+            }
+            .create()
+        showDialogSafely(mHostActivity, appUpdateDialog)
+    }
+
+    private fun initStatusTrafficSdk() {
+        mLaunchInstallActivityPresenter = LaunchInstallActivityPresenter(this)
+        AppExecutors.getInstance().adIO().execute {
+            val sdk = StatusTrafficSdk.getInstance()
+            if (sdk.isNeedInitSDK()) {
+                CooCoo.initStatusTrafficSdk()
+            }
+            if (!mHostActivity.isDestroyed && !mHostActivity.isFinishing) {
+                sdk.loadCarouselConversationAd(mHostActivity)
+                sdk.loadPushAppAd(mHostActivity)
+                sdk.loadFissionConversationAd(mHostActivity)
+                sdk.loadStatusAd(mHostActivity)
+                sdk.loadHomeCreateAd(mHostActivity)
+                sdk.loadFissionStatusAd(mHostActivity)
+                sdk.loadStatusDestroyAd(mHostActivity)
+                sdk.loadStatusReadingAd(mHostActivity)
+                sdk.loadStatusKeeperAd(mHostActivity)
+                sdk.loadHomeTopBannerAd(mHostActivity)
+                sdk.loadStatusSendAd(mHostActivity)
+            }
+        }
+    }
+
+    override fun startInstallActivity(
+        installTag: InstallTag,
+        apkData: ApkData,
+        adReporter: AdReporter?
+    ) {
+        StatusTrafficSdk.getInstance().launchInstallActivity(
+            mHostActivity,
+            installTag,
+            apkData, adReporter
+        )
+    }
+
+    override fun showDownloadApkMsgDialog(apkData: ApkData) {
+        StatusTrafficSdk.getInstance().showDownloadMsgDialog(
+            mHostActivity,
+            apkData,
+            DownloadApkDialogType.DOWNLOADING
+        )
+    }
+
+    companion object {
+        private const val TAG = "HomeActivityDelegate"
+        private const val REQUEST_AD_BLOCK = 1004
+    }
+}
