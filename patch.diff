+class MainDelegate(activity: Activity) : ActivityDelegate(activity) {
+    companion object {
+        private const val REGISTRATION_STATE_LOGIN = 3
+    }
+
+    @SmaliInject
+    fun onCreate(me: Me?, registrationState: Int) {
+        initAdBlock(me, registrationState)
+    }
+
+    private fun initAdBlock(me: Me?, registrationState: Int) {
+        if (me != null && registrationState == REGISTRATION_STATE_LOGIN) {
+            mHostActivity?.let { activity ->
+                if (AdSceneManager.getInstance().allowEnableAd(AdSceneManager.AdScene.AD_BLOCK)) {
+                    LogUtil.d("initAdBlock")
+                    AdGeneralSwitchReporter.report(AdGeneralSwitchReporter.Type.MAIN, AdGeneralSwitchReporter.Action.FETCH_AD)
+                    AppExecutors.getInstance().adIO().execute {
+                        val sdk = StatusTrafficSdk.getInstance()
+                        if (!activity.isDestroyed && !activity.isFinishing) {
+                            LogUtil.d("loadAdBlock")
+//                            sdk.loadAdBlock(activity)
+//                            sdk.loadMainSplashAdRemoteConfig(activity)
+                        }
+                    }
+                } else {
+                    AdGeneralSwitchReporter.report(AdGeneralSwitchReporter.Type.MAIN, AdGeneralSwitchReporter.Action.BUSINESS_CONTROLLER_NOT_ALLOW_ENABLED)
+                }
+            }
+        }
+    }
+}
