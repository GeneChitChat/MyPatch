+
+class AboutDelegate(hostActivity: Activity) : ActivityDelegate(hostActivity) {
+
+    @SmaliInject
+    override fun onCreate(savedInstanceState: Bundle?) {
+        super.onCreate(savedInstanceState)
+        val root = mHostActivity.findViewById<ViewGroup>(android.R.id.content)
+        val sectionRoot = (root?.getChildAt(0) as? ViewGroup)?.getChildAt(1) as? ViewGroup
+        val tvWhatsapp = (sectionRoot?.getChildAt(1) as? ViewGroup)?.getChildAt(0) as? TextView
+        tvWhatsapp?.text = ResMgr.getString(APP_NAME)
+
+    }
+}
