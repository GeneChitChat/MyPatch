+package com.coocoo.whatsappdelegate
+
+import android.app.Activity
+import android.content.ContextWrapper
+import android.view.View
+import com.coocoo.annotation.SmaliInject
+import com.coocoo.exoplayer2.util.Log
+import com.coocoo.statuses.myhistory.MyStatusHistoryPlaybackActivity
+
+object MyStatusDeleteBtnDelegate {
+    @SmaliInject
+    @JvmStatic
+    fun onViewCreated(btnDelete: View) {
+
+        var activity = btnDelete.context
+        while (activity is ContextWrapper) {
+           activity = activity.baseContext
+        }
+        Log.d("MyStatusDeleteBtnDelegate", "onViewCreated: $activity")
+        if (activity is MyStatusHistoryPlaybackActivity) {
+            btnDelete.setOnClickListener {
+                activity.showConfirmDeleteDialog()
+            }
+        }
+    }
+}
