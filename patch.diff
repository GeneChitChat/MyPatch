+package com.coocoo.whatsappdelegate
+
+import android.content.Intent
+import android.content.pm.PackageManager
+import android.net.Uri
+import android.os.Bundle
+import android.os.Handler
+import android.view.View
+import android.view.ViewGroup
+import android.widget.TextView
+import com.coocoo.annotation.SmaliInject
+import com.coocoo.utils.Constants.WHATS_APP_URL
+import com.coocoo.utils.LogUtil
+import com.coocoo.utils.ResMgr
+import com.coocoo.whatsappdelegate.base.ActivityDelegate
+import com.gbwhatsapp.youbasha.ui.YoSettings.AllSettings
+
+class AllSettingsDelegate(hostActivity: AllSettings) : ActivityDelegate(hostActivity) {
+
+    @SmaliInject
+    fun setWhatsAppClickListener(view: View) {
+        val title = ((view as ViewGroup).getChildAt(1) as ViewGroup).getChildAt(0) as TextView
+        val packageInfo = view.context.packageManager.getPackageInfo(view.context.packageName, 0)
+        val str = "${ResMgr.getString("cc_app_name")} ${packageInfo.versionName}"
+        LogUtil.d(str)
+        title.text = str
+        view.setOnClickListener {
+            val intent = Intent(Intent.ACTION_VIEW, Uri.parse(WHATS_APP_URL))
+            mHostActivity.startActivity(intent)
+        }
+    }
+}
